					p #{xmlQWvalue[i]}
					-}
		script(
			type="text/javascript"
			src="static/js/jquery-1.12.1.min.js"
		)
		script(
			type="text/javascript"
			src="static/js/echarts.common.min.js"
		)
		script(
			type="text/javascript"
			src="static/js/main.js"
		)
		script(
			type="text/javascript"
			src="static/js/examine.js"
		)

		
			
		